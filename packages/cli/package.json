{
  "name": "@tscad/cli",
  "version": "0.4.0",
  "description": "A CLI tool for working with tscad models",
  "homepage": "https://tscad.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/tscad.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "tscad": "./out/bin.js"
  },
  "scripts": {
    "build": "rslib build",
    "check:exports": "publint",
    "check:types": "tsc --noEmit",
    "dev": "rslib dev --watch",
    "dev:on-success": "PORT=3999 node out/bin.js dev ../../examples/gridfinity-generator/src/model.ts",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@jscad/3mf-serializer": "^2.1.15",
    "@jscad/obj-serializer": "^2.1.21",
    "@jscad/stl-serializer": "^2.1.21",
    "@tscad/viewer": "workspace:*",
    "@vitejs/plugin-react": "^5.1.1",
    "change-case": "^5.4.4",
    "commander": "^14.0.2",
    "debug": "^4.4.3",
    "esbuild": "^0.25.12",
    "open": "^10.2.0",
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@rslib/core": "^0.17.2",
    "@toolsync/template": "^0.1.1",
    "@tscad/commander": "workspace:*",
    "@tscad/modeling": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "publint": "catalog:",
    "rsbuild-exec": "workspace:*",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
