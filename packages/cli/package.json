{
  "name": "@tscad/cli",
  "version": "0.1.1",
  "description": "A CLI tool for working with tscad models",
  "homepage": "https://tscad.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/tscad.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "tscad": "./out/bin.js"
  },
  "scripts": {
    "build": "tsup",
    "build:readme": "pnpm update-section README.md usage \"\\`\\`\\`\n$(node out/bin.js --help)\n\\`\\`\\`\"",
    "check:exports": "publint",
    "check:types": "tsc --noEmit",
    "dev": "tsup --watch",
    "dev:on-success": "PORT=3999 node out/bin.js dev ../../examples/gridfinity-generator/src/model.ts",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@tscad/viewer": "workspace:*",
    "@vitejs/plugin-react": "^5.0.0",
    "commander": "^14.0.0",
    "debug": "^4.4.1",
    "open": "^10.2.0",
    "vite": "^7.1.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@toolsync/template": "^0.1.1",
    "@tscad/commander": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.15.32",
    "eslint": "^9.29.0",
    "publint": "^0.3.12",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
