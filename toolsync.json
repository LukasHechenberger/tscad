{
  "@toolsync/builtin/github-actions": {},
  "@toolsync/builtin/ignore-sync": {},
  "@toolsync/builtin/package-meta": {
    "publishConfig": {
      "access": "public"
    }
  },
  "@toolsync/builtin/package-readme": {},
  "@toolsync/builtin/turbo": {},
  "@toolsync/builtin/pnpm": {},
  "@toolsync/builtin/prettier": {
    "settings": {
      "printWidth": 100,
      "singleQuote": true,
      "plugins": ["prettier-plugin-packagejson"]
    }
  },
  "@toolsync/builtin/vscode": {
    "recommendations": ["mtsmfm.vscode-jscad-preview"],
    "settings": {
      "eslint.workingDirectories": ["examples/*", "apps/*", "packages/*"],
      "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
      }
    }
  }
}
