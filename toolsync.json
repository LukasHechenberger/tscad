{
  "@toolsync/builtin/github-actions": {},
  "@toolsync/builtin/ignore-sync": {},
  "@toolsync/builtin/package-meta": {
    "publishConfig": {
      "access": "public"
    }
  },
  "@toolsync/builtin/package-readme": {},
  "@toolsync/builtin/turbo": {
    "tasks": {
      "test:integration": {
        "dependsOn": ["build"]
      }
    }
  },
  "@toolsync/builtin/pnpm": {},
  "@toolsync/builtin/prettier": {
    "settings": {
      "printWidth": 100,
      "singleQuote": true,
      "tsdoc": true,
      "plugins": ["prettier-plugin-packagejson", "prettier-plugin-jsdoc"]
    }
  },
  "@toolsync/builtin/vscode": {
    "settings": {
      "eslint.workingDirectories": ["examples/*", "apps/*", "packages/*"],
      "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
      }
    }
  },
  "@toolsync/builtin/publint": {},
  "@repo/toolsync-plugin": {}
}
